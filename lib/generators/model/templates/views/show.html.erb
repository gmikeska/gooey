<%= "<% if(notice) %%>" %>
<%= "<div class='alert alert-primary' role='alert' id='notice' style='margin-top:1%;'><%= notice %%></div>" %>
<%= "<% end %%>" %>

<% editable_attributes.each do |attribute| %>
<p>
  <%= "<strong>#{attribute.name}</strong>" %>
  <%= "<%= #{singular_name}.#{attribute.name} %%>" %>
</p>
<% end -%>

<%= "<%= link_to 'Edit', edit_#{singular_name}_path(@#{singular_name}) %%> | <%= link_to 'Back', #{singular_name}_path %%>" %>
